package-as: totalRP3
enable-nolib-creation: no

ignore:
  - README.md
  - CONTRIBUTING.md
  - CODE_OF_CONDUCT.md
  - LICENSE
  - Scripts
  - totalRP3/libs/LibDeflate/.appveyor
  - totalRP3/libs/LibDeflate/.rockspecs
  - totalRP3/libs/LibDeflate/.travis
  - totalRP3/libs/LibDeflate/docs
  - totalRP3/libs/LibDeflate/examples
  - totalRP3/libs/LibDeflate/tests
  - totalRP3/libs/LibRPMedia/Exporter
  - totalRP3/libs/LibRPMedia/Makefile
  - totalRP3/libs/LibRPMedia/Tests
  - totalRP3/libs/LibRPMedia/UI

embedded-libraries:
  - ace3
  - callbackhandler
  - libchatanims
  - libcompress
  - libdatabroker-1-1
  - libdbicon-1-0
  - libdeflate
  - librealminfo
  - librpmedia
  - libstub
  - msa-dropdownmenu-10
  - utf8

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown

move-folders:
  totalRP3/totalRP3: totalRP3
  totalRP3/totalRP3_Data: totalRP3_Data
